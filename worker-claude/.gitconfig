[user]
    name = Joshua Clark
    email = josh@clarkpdx.com
[push]
    autoSetupRemote = true
[init]
    defaultBranch = main
[core]
    pager = delta
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true
    side-by-side = true
[merge]
    conflictstyle = diff3
[diff]
    colorMoved = default
